{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}\\build\\src\\index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Migration File",
      "program": "${workspaceFolder}\\build\\src\\migration\\index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha Tests",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "-r",
        "ts-node/register",
        "--config",
        "test/.mocharc.json",
        "--slow",
        "5000",
        "test/**/*.test.ts"
      ],
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": "Migrate Historical zAuction Bids",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register/transpile-only"],
      "sourceMaps": true,
      "program": "${workspaceFolder}\\build\\scripts\\migrateHistoricBids.js",
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"],
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
